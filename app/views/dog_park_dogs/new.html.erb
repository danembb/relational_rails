<%= form_with url:"/dog_parks/#{@dog_park.id}/dogs", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name  %>
  <%= form.label :plays_fetch %>
  <%= form.text_field :plays_fetch  %>
  <%= form.label :age %>
  <%= form.text_field :age  %>
  <%= form.label :breed %>
  <%= form.text_field :breed  %>
  <%= form.hidden_field :dog_park_id, value: @dog_park.id %>
  <%= form.submit "Create Dog Park" %>
<% end %>
